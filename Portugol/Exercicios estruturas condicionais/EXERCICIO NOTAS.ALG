Algoritmo "Exercicio notas"

Var

nota1, nota2, soma : real

Inicio
   escreva("Digite a primeira nota: ")
   leia(nota1)
   escreva("Digite a segunda nota: ")
   leia(nota2)
   soma <- nota1 + nota2
   escreval("Nota final = ", soma:1:1)
   se soma > 60.0 entao
      escreval("Aprovado")
   senao
      escreval("reprovado")
   fimse
   


Fimalgoritmo